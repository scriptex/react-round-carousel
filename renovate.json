{
	"extends": [
		"config:base",
		":automergePatch",
		":automergeMinor",
		":automergeBranch",
		":disableDependencyDashboard"
	],
	"travis": {
		"enabled": true
	},
	"assignees": ["@scriptex"],
	"labels": ["dependencies"],
	"rebaseWhen": "conflicted",
	"vulnerabilityAlerts": {
		"labels": ["security"],
		"assignees": ["@scriptex"]
	},
	"rangeStrategy": "replace",
	"major": {
		"automerge": false
	}
}
